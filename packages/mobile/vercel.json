{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@expo/cli@latest",
      "config": {
        "experimental": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ENV": "production"
  }
}
